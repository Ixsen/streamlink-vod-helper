<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.ixsen.streamlinkvodhelper.view.PlayVideoPopupController">
    <children>
        <VBox>
            <children>
                <ComboBox fx:id="quality" prefWidth="200.0" promptText="Choose quality">
                    <VBox.margin>
                        <Insets left="5.0" right="5.0" top="5.0"/>
                    </VBox.margin>
                </ComboBox>
                <HBox>
                    <children>
                        <Button fx:id="playButton" mnemonicParsing="false" text="Play">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" top="5.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="cancelButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false"
                                text="Cancel">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
